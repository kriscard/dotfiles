      "  switch (action.type) {",
      "    case '${5:ACTION_TYPE}':",
      "      return { ...state, ${8:// update logic} }",
      "    default:",
      "      return state",
      "  }",
      "}",
      "",
      "const [state, dispatch] = useReducer(${7:reducer}, ${9:initialState})"
    ],
    "description": "useReducer hook with typed state and actions"
  },
